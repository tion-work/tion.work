# Server Configuration
PORT=8080
GIN_MODE=release

# External Database Configuration
# 使用外部PostgreSQL数据库
DATABASE_URL=postgres://username:password@your-db-host:5432/tion_db?sslmode=disable

# External Redis Configuration
# 使用外部Redis服务
REDIS_URL=redis://your-redis-host:6379/0

# API Configuration
API_KEY=your-secret-api-key-here

# Service Configuration
SERVICE_NAME=Tion Backend API
VERSION=1.0.0

# 示例配置：
# DATABASE_URL=postgres://postgres:password@localhost:5432/tion_db?sslmode=disable
# REDIS_URL=redis://localhost:6379/0
# API_KEY=your-secret-api-key-here
